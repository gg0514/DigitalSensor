    </LinearLayout>
    <LinearLayout
        android:id="@+id/Trend_Flow_X"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Trend_Flow_X_Text_Title"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_x_zoom_in_out"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_weight="50"/>
        <Button
            android:id="@+id/Trend_Flow_X_Btn_In"
            android:text="@string/multi_in"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="25"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="#FFFFFF"
            android:background="@drawable/my_button_base" />
        <Button
            android:id="@+id/Trend_Flow_X_Btn_Out"
            android:text="@string/multi_out"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="25"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="#FFFFFF"
            android:background="@drawable/my_button_base" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/Trend_Flow_Previous"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Trend_Flow_Previous_Text_Title"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_previous_data"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_weight="50"/>

        <Button
            android:id="@+id/Trend_Flow_Previous_Btn_Left"
            android:text="◀"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="25"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="#FFFFFF"
            android:background="@drawable/my_button_base" />

        <Button
            android:id="@+id/Trend_Flow_Previous_Btn_Right"
            android:text="▶"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="25"
            android:layout_marginBottom="5dp"
