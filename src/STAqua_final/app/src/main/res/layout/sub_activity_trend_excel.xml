<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2c2d35" >

    <LinearLayout
        android:id="@+id/Trend_EX_Content"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Trend_EX_Content_Text"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_trend_excel"
            android:layout_marginLeft="15dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:layout_weight="100"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Trend_EX_Dummy1"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
    </LinearLayout>
    <LinearLayout
        android:id="@+id/Trend_EX_Dummy2"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
    </LinearLayout>
    <LinearLayout
        android:id="@+id/Trend_EX_Status"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">

        <TextView
            android:id="@+id/Trend_EX_Status_Text1"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="30"
            android:gravity="center_vertical|left"
            android:padding="10dp"
            android:text="@string/multi_trend_count"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/Trend_EX_Status_Text2"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="10"
            android:gravity="center_vertical|left"
            android:padding="10dp"
            android:text=":"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/Trend_EX_Status_Text3"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="60"
            android:gravity="center_vertical|left"
            android:padding="10dp"
            android:text=""
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:visibility="visible" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Trend_EX_Title1"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <FrameLayout
            android:orientation="horizontal"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="100">
            <TextView
                android:id="@+id/Trend_EX_Title1_Text"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:text="@string/multi_export_trend_data"
                android:gravity="center"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_weight="100"/>

            <ProgressBar
                android:id="@+id/Trend_EX_Title1_ProgressBar"
                style="@android:style/Widget.ProgressBar.Horizontal"
                android:progressDrawable="@drawable/progressbar_progressbar"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="center"
                android:padding="15dp"/>
        </FrameLayout>
    </LinearLayout>

