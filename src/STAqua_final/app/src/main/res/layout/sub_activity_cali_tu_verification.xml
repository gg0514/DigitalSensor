<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Cali_Tu_Ver_Main"
android:orientation="vertical"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#1a1b20"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <LinearLayout
        android:id="@+id/Cali_Tu_Ver_Content"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Cali_Tu_Ver_Content_Text"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_cal_tu_ver"
            android:layout_marginLeft="15dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:layout_weight="100"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Cali_Tu_Ver_Stick"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Cali_Tu_Ver_Stick_Text"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_stick_no"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_weight="50"/>
        <EditText
            android:id="@+id/Cali_Tu_Ver_Stick_Ed"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="1"
            android:textColor="#38C6C8"
            android:padding="10dp"
            android:ems="10"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="#141418"
            android:layout_weight="30"
            android:inputType="number" />

        <Button
            android:id="@+id/Cali_Tu_Ver_Stick_Btn"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="20"
            android:background="@drawable/my_button_base"
            android:text="@string/multi_edit"
            android:textColor="#FFFFFF"
            android:visibility="invisible" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Cali_Tu_Ver_Date"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Cali_Tu_Ver_Date_Text"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_setting_date"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_weight="50"/>
        <EditText
            android:id="@+id/Cali_Tu_Ver_Date_Ed"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text=""
            android:textColor="#38C6C8"
            android:padding="10dp"
            android:ems="10"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="#2f2f35"
            android:focusableInTouchMode="false"
            android:layout_weight="50"
            android:inputType="date" />
    </LinearLayout>

    <!--<LinearLayout-->
        <!--android:id="@+id/Cali_Tu_Ver_Dummy1"-->
        <!--android:orientation="horizontal"-->
        <!--android:background="#2c2d35"-->
        <!--android:layout_width="fill_parent"-->
        <!--android:layout_height="0px"-->
        <!--android:layout_weight="10">-->
    <!--</LinearLayout>-->


    <LinearLayout
        android:id="@+id/Cali_Tu_Ver_Standard"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Cali_Tu_Ver_Standard_Text"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_standard"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_weight="50"/>

        <EditText
            android:id="@+id/Cali_Tu_Ver_Standard_Ed"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="0.000"
            android:textColor="#38C6C8"
            android:padding="10dp"
            android:ems="10"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="#2f2f35"
            android:layout_weight="30"
            android:inputType="textPersonName"
            android:focusableInTouchMode="false"/>

        <Button
            android:id="@+id/Cali_Tu_Ver_Standard_Btn"
            android:text="@string/multi_meas"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="20"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="#FFFFFF"
            android:background="@drawable/my_button_base" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Cali_Tu_Ver_Measured"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Cali_Tu_Ver_Measured_Text"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_measured"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_weight="50"/>

        <EditText
            android:id="@+id/Cali_Tu_Ver_Measured_Ed"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="0.000"
            android:textColor="#38C6C8"
            android:padding="10dp"
            android:ems="10"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="#2f2f35"
            android:layout_weight="30"
            android:inputType="textPersonName"
            android:focusableInTouchMode="false"/>

        <Button
            android:id="@+id/Cali_Tu_Ver_Measured_Btn"
            android:text="@string/multi_meas"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="20"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="#FFFFFF"
            android:background="@drawable/my_button_base" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Cali_Tu_Ver_Rate"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="10">
        <TextView
            android:id="@+id/Cali_Tu_Ver_Rate_Text"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="@string/multi_std_rate"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_weight="50"/>

        <EditText
            android:id="@+id/Cali_Tu_Ver_Rate_Ed"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="0.00"
            android:textColor="#38C6C8"
            android:padding="10dp"
            android:ems="10"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="#2f2f35"
            android:focusableInTouchMode="false"
            android:layout_weight="40"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/Cali_Tu_Ver_Rate_Text1"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:text="%"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_weight="10"/>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/Cali_Tu_Ver_Help"
        android:orientation="horizontal"
        android:background="#2c2d35"
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="30">

        <FrameLayout
            android:id="@+id/Cali_Tu_Ver_Help_Icon"
            android:orientation="horizontal"
            android:background="#2c2d35"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="15">

            <ImageView
                android:id="@+id/Cali_Tu_Ver_Help_Icon_Beaker"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:scaleType="fitCenter"
                android:layout_marginLeft="10dp"
                android:background="#212128"
                android:color="#4d4e53"
                android:src="@drawable/ic_exclamation"/>
            <ProgressBar
                android:id="@+id/Cali_Tu_Ver_Help_Progress"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:scaleType="fitCenter"
                android:layout_marginLeft="10dp"
                android:visibility="invisible"
                android:background="#212128"
                android:layout_weight="100"/>
        </FrameLayout>

        <LinearLayout
            android:id="@+id/Cali_Tu_Ver_Help_Text"
            android:orientation="vertical"
            android:background="#2c2d35"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="85">
            <TextView
                android:id="@+id/Cali_Tu_Ver_Help_Text1"
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:text=""
                android:gravity="center_vertical"
                android:padding="10dp"
                android:layout_marginRight="10dp"
                android:background="#212128"
                android:textColor="#4d4e53"
                android:textSize="15sp"
                android:layout_weight="100"/>
        </LinearLayout>
    </LinearLayout>
